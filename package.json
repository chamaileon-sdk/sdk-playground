{
	"name": "sdk-playground",
	"version": "2.9.0",
	"scripts": {
		"build": "vite build",
		"dev": "npm run serve",
		"lint": "npx eslint . --ext .js,.mjs,.cjs,.json,.vue  --resolve-plugins-relative-to ./node_modules/@chamaileon-sdk/eslint-config",
