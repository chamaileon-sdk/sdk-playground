		"vue": "^2.7.16",
		"vue-color": "^2.8.1",
		"vue-highlight.js": "^4.0.1-beta",
		"vue-inline-svg": "^2.1.4",
		"vue-meta": "^2.4.0",
		"vue-observe-visibility": "^1.0.0",
		"vue-router": "^3.6.5",
		"vue-scrollactive": "^0.9.3",
		"vuedraggable": "^2.24.3",
		"vuetify": "^2.7.2",
		"vuex": "^3.6.2"
